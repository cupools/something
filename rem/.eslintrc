{
  "root": true,  
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "commonjs": true
  },
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "no-use-before-define": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "no-mixed-operators": 0,
    "import/no-unresolved": 0
  }
}